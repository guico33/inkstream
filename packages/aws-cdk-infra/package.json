{
  "name": "@inkstream/aws-cdk-infra",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "cdk:synth:dev": "cdk synth --profile dev --context environment=dev",
    "cdk:deploy:dev": "cdk deploy --profile dev --context environment=dev --all",
    "cdk:deploy:backend:dev": "cdk deploy Dev-InkstreamBackendStack --profile dev --context environment=dev",
    "cdk:deploy:frontend:dev": "cdk deploy Dev-InkstreamFrontendStack --profile dev --context environment=dev",
    "cdk:diff:dev": "cdk diff --profile dev --context environment=dev",
    "cdk:diff:backend:dev": "cdk diff Dev-InkstreamBackendStack --profile dev --context environment=dev",
    "cdk:diff:frontend:dev": "cdk diff Dev-InkstreamFrontendStack --profile dev --context environment=dev",
    "cdk:destroy:dev": "cdk destroy --profile dev --context environment=dev --all",
    "cdk:synth:prod": "DOTENV_CONFIG_PATH=.env.prod cdk synth --profile prod --context environment=prod",
    "cdk:deploy:prod": "DOTENV_CONFIG_PATH=.env.prod cdk deploy --profile prod --context environment=prod --all",
    "cdk:deploy:backend:prod": "DOTENV_CONFIG_PATH=.env.prod cdk deploy Prod-InkstreamBackendStack --profile prod --context environment=prod",
    "cdk:deploy:frontend:prod": "DOTENV_CONFIG_PATH=.env.prod cdk deploy Prod-InkstreamFrontendStack --profile prod --context environment=prod",
    "cdk:diff:prod": "DOTENV_CONFIG_PATH=.env.prod cdk diff --profile prod --context environment=prod",
    "cdk:diff:backend:prod": "DOTENV_CONFIG_PATH=.env.prod cdk diff Prod-InkstreamBackendStack --profile prod --context environment=prod",
    "cdk:diff:frontend:prod": "DOTENV_CONFIG_PATH=.env.prod cdk diff Prod-InkstreamFrontendStack --profile prod --context environment=prod",
    "cdk:destroy:prod": "DOTENV_CONFIG_PATH=.env.prod cdk destroy --profile prod --context environment=prod --all",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:workflow": "vitest run --config vitest.workflow.config.ts --reporter=verbose",
    "test:workflow:concurrent": "VITEST_CONCURRENT=true vitest run --config vitest.workflow.config.ts --reporter=verbose",
    "get-test-user-token": "ts-node get-test-user-token.ts",
    "migrate": "ts-node --project tsconfig.migrations.json run-migrations.ts",
    "migrate:dry-run": "ts-node --project tsconfig.migrations.json run-migrations.ts --dry-run",
    "migrate:verbose": "ts-node --project tsconfig.migrations.json run-migrations.ts --verbose",
    "migrate:dry-run:verbose": "ts-node --project tsconfig.migrations.json run-migrations.ts --dry-run --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.812.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.812.0",
    "@aws-sdk/client-dynamodb": "^3.812.0",
    "@aws-sdk/client-polly": "^3.812.0",
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/client-secrets-manager": "^3.817.0",
    "@aws-sdk/client-sfn": "^3.812.0",
    "@aws-sdk/client-textract": "^3.812.0",
    "@aws-sdk/credential-provider-ini": "^3.812.0",
    "@aws-sdk/lib-dynamodb": "^3.814.0",
    "@inkstream/shared": "1.0.0",
    "aws-cdk-lib": "^2.196.0",
    "constructs": "^10.4.2",
    "dotenv": "^16.5.0",
    "dynamodb-toolbox": "^2.3.0",
    "openai": "^4.103.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@vitest/coverage-v8": "^3.1.4",
    "aws-cdk": "^2.1016.0",
    "aws-sdk-client-mock": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
